<FindBugsFilter>
	
	<!-- All bugs in generated packages -->
	<Match>
		<Package name="~.*generated.*" />
	</Match>
	
	<Match>
		<Package name="~.*com.cryptlex.lexactivator.*" />
	</Match>

	<!-- All bugs in test classes, except for JUnit-specific bugs -->
	<Match>
		<Class name="~.*\.*Test" />
		<Not>
			<Bug code="IJU" />
		</Not>
	</Match>

</FindBugsFilter>